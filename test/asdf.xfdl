    <Script type="xscript5.1"><![CDATA[
this.btn_alert_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var sMsgId = "msg.err.validator.minlength";	 	//메세지ID
	var arrArg = ["이름","3"];						//메세지취환될값 배열[생략가능]
	var sPopId = sMsgId;							//메세지팝업ID[생략가능]	*해당화면에서 메시지 중복사용시 구분되는값을 넣어줘야함
	var sMsgCallback = "";							//메세지콜백[생략가능] 		* confirm성 메시지를 사용 시 반드시 필요
	
	this.gfnAlert(sMsgId, arrArg, sPopId, sMsgCallback);
};

this.btn_Confirm_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var sMsgId = "confirm.logout";	 	//메세지ID
	var arrArg = "";								//메세지취환될값 배열[생략가능]
	var sPopId = sMsgId;							//메세지팝업ID[생략가능]	*해당화면에서 메시지 중복사용시 구분되는값을 넣어줘야함
	var sMsgCallback = "fn_callback";							//메세지콜백[생략가능] 		* confirm성 메시지를 사용 시 반드시 필요
	
	this.gfnAlert(sMsgId, arrArg, sPopId, sMsgCallback);
};

this.fn_callback = function (id, rtn)
{
	if(rtn) {
		this.alert("확인");
	} else {
		this.alert("취소");
	}
	
};
this.Button02_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.transaction(
		"svcGetEmpList",
		"SvcUrl::nexa/getEmpList.do",
		"",
		"ds_emp=out_emp",
		"",
		""	
	);
};
]]></Script>
