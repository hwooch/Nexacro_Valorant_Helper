  <CSSData type="mobile_small"><![CDATA[/*************** MainFrame ***************/
.MainFrame
{
	font : 20px "NotoSansKR-Regular", -apple-system, BlinkMacSystemFont ;	
}
/*************** Form ***************/
.Form
{
	color : #555555;
/*	font : -nexa-define(--Mdefault20);*/		
	font : 20px "NotoSansKR-Regular", -apple-system, BlinkMacSystemFont ;	
}
/*************** ScrollIndicatorControl ***************/
.VScrollIndicatorControl
{
	-nexa-border : 0px none,0px none,0px none,1px solid #acacac;
	background : #ebebeb;
	-nexa-padding : 0px 3px;	
}
.VScrollIndicatorControl .trackbar
{
	background : #c2c2c2;
}
/*************** Edit / EditControl ***************/
.Edit,
.EditControl
{
	-nexa-padding : 0px 10px ;		
	border-radius : 5px;
}
/*************** MaskEdit / MaskEditControl ***************/
.MaskEdit,
.MaskEditControl
{
	-nexa-padding : 0px 10px ;		
	border-radius : 5px;
}
/*************** TextArea / TextAreaControl ***************/
.TextArea,
.TextAreaControl
{
	-nexa-padding : 5px 10px ;
	border-radius : 5px;
	font : -nexa-define(--Mlineheight20);	
}
/*************** Button ***************/
.Button,
.Button[status=mouseover],
.Button[status=focused]
{
	-nexa-border : 1px solid #2f59bf;
	-nexa-padding : 0px;
	background : #2f59bf;
	border-radius : 5px;	
	color : #ffffff;
}
.Button[userstatus=pushed],
.Button[userstatus=selected]
{
	-nexa-border : 1px solid #002795;
	background : #002795;
	color : #ffffff;
}
/*************** CheckBox ***************/
