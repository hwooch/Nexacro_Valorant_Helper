    <Script type="xscript5.1"><![CDATA[this.fvApp = nexacro.getApplication();

this.valo1002fm_onclose = function(obj:nexacro.Form,e:nexacro.CloseEventInfo)
{
	this.fvApp.gds_login.removeEventHandler("onrowsetchanged", this.fn_LoginDataChanged, this);
	this.fvApp.gds_login.removeEventHandler("onload", this.fn_LoginDataChanged, this);
	
	// 테마 이벤트 핸들러 제거
    this.fvApp.gds_theme.removeEventHandler("oncolumnchanged", this.fn_change_theme, this);
};

// 글로벌 데이터셋(gds_login) 변경 시
this.fn_LoginDataChanged = function(obj:nexacro.NormalDataset, e:nexacro.DSRowsetChangeEventInfo)
{
    // 1. 로그인이 된 경우 (데이터가 생김)
    if (obj.getRowCount() > 0) {
        // 아까 만든 로그인 상태 갱신 함수 호출
        this.fn_setLoginState(); 
    }
    // 2. 로그아웃 된 경우 (데이터가 사라짐)
    else {
        // 아까 만든 로그아웃 처리 함수 호출
        this.fn_setLogoutState();
    }
};


// 로그인 성공 시 호출할 함수
this.fn_setLoginState = function()
{
    // 1. 로그인 여부 재확인
    if(this.fvApp.gds_login.getRowCount() == 0) return;
	
    var sLoginDiscordId = this.fvApp.gds_login.getColumn(0, "DISCORD_ID");
	
    // 2. DB에서 저장된 목록 다시 가져오기 (비동기)
    // -> 완료되면 fn_Callback의 'getSavedList' 부분에서 별을 다시 칠해줌
    var strSvcID        = "getSavedList";
    var strURL          = "SvcUrl::selectSavedMatchIds.do";
    var strInDatasets   = "";
    var strOutDatasets  = "ds_saved_ids=out_saved_ids";
    var strArgument     = "discord_id=" + nexacro.wrapQuote(sLoginDiscordId);
    var strCallbackFunc = "fn_Callback";
    
    this.transaction(strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc);
};

// 로그아웃 시 호출할 함수
this.fn_setLogoutState = function()
{
    this.list_match_list.enableevent = false;
	
    // 1. 저장된 목록 데이터셋 비우기
    this.ds_saved_ids.clearData();
	
    // 2. 화면의 모든 별을 '빈 별'로 초기화
    for(var i=0; i<this.ds_list.getRowCount(); i++)
    {
        this.ds_list.setColumn(i, "STAR_CSS", "star_empty");
    }
	
    this.list_match_list.enableevent = true;
};

// 화면 로드시에 이름 입력했던걸 기반으로 전적 검색
this.valo1002fm_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	// gds_login 데이터셋 변경 감지 이벤트 등록
    // gds_login의 행 개수가 바뀌거나 데이터가 바뀌면 fn_LoginDataChanged 함수를 실행
    this.fvApp.gds_login.addEventHandler("onrowsetchanged", this.fn_LoginDataChanged, this);
	this.fvApp.gds_login.addEventHandler("onload", this.fn_LoginDataChanged, this);
	
	this.scrollbartype = "none none";
	// 혹시모를 데이터 지우기
	this.ds_tier.clearData();
    this.ds_list.clearData();
    this.ds_stats.clearData();
    this.ds_role.clearData();
    this.ds_agent.clearData();
    this.ds_weapon.clearData();
	this.ds_chart_win_rate.clearData();
	this.ds_saved_ids.clearData();
	
	// 검색 중 화면 표시하기
	this.sta_hide_screen.visible = true;
	this.btn_space_2.visible = true;
	
    var username = this.fvApp.getVariable("gv_Recent_Search_Name");
    var usertag  = this.fvApp.getVariable("gv_Recent_Search_Tag");
    
    // 메인화면 이름, 태그 설정
    this.div_user_status.form.sta_user_name.text = username;
    this.div_user_status.form.sta_user_tag.text = " #" + usertag;
	
	// 로딩 중 상태 표시 
	this.div_user_status.form.sta_user_status.text = "티어 조회 중...";
    this.div_user_status.form.sta_user_status.background = "";
    this.div_user_status.form.resetScroll();
	
	// 전적 조회 transaction
    var strSvcID        = "searchMatch";
    var strURL          = "SvcUrl::getValorantMatches.do";
    var strInDatasets   = "";
    var strOutDatasets  = "ds_list=out_list ds_stats=out_stats ds_role=out_role ds_agent=out_agent ds_weapon=out_weapon";
    var strArgument     = "vName=" + nexacro.wrapQuote(username) + " vTag=" + nexacro.wrapQuote(usertag);
    var strCallbackFunc = "fn_Callback";
	
    this.transaction(strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc);
	
    // 티어 조회 transaction
    var strSvcID        = "searchTier";
    var strURL          = "SvcUrl::getValorantTier.do";
    var strInDatasets   = "";
    var strOutDatasets  = "ds_tier=out_tier";
    var strArgument     = "vName=" + nexacro.wrapQuote(username) + " vTag=" + nexacro.wrapQuote(usertag);
    var strCallbackFunc = "fn_Callback";
	
    this.transaction(strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc);	
	
	// 요원 정보 조회 transaction
	var strSvcID        = "getAgents";
    var strURL          = "SvcUrl::getAgentList.do";
    var strInDatasets   = "";
    var strOutDatasets  = "ds_agent_info=out_agent_info";
	// ds_agent_info 전적 조회에서 비동기로 데이터를 추가해서 받는거라 안쓰긴 함. 그냥 일단 받아만 놨음
    var strArgument     = "";
    var strCallbackFunc = "fn_Callback";
	
    this.transaction(strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc);
	
	
	// 로그인 했을 때만 조회
    if(this.fvApp.gds_login.getRowCount() > 0) {
        this.fn_setLoginState();
    }
	
	// 테마 변경 이벤트 핸들러 추가
    this.fvApp.gds_theme.addEventHandler("oncolumnchanged", this.fn_change_theme, this);
    
    // 최초 로드시 현재 저장된 테마를 강제로 한 번 적용
    this.fn_setThemeCss();
};

this.fn_change_theme = function(obj:nexacro.NormalDataset, e:nexacro.DSColChangeEventInfo)
{
    // 데이터셋이 변경되었을 때 CSS 적용 함수 호출
    this.fn_setThemeCss();
};

// 실제 CSS를 변경하는 로직
this.fn_setThemeCss = function()
{
    // 현재 데이터셋에 설정된 값 가져오기
    var sThemeColor = this.fvApp.gds_theme.getColumn(0, "COLOR");
    
    // 값에 따라 스타일 적용
    if (sThemeColor == "white") {
        nexacro.loadStyle("xcssrc::theme_white.xcss");
		
		this.list_statistics.setCellProperty("body", "cell_devide", "background", 
            "#cccccc");
		this.list_role.setCellProperty("body", "CellLine", "background", 
			"#cccccc");
		this.list_agent.setCellProperty("body", "CellLine", "background", 
			"#cccccc");
		this.list_weapon.setCellProperty("body", "CellLine", "background", 
			"#cccccc");
		
		var ds = this.ds_list;
		
		for (var i = 0; i < ds.getRowCount(); i++) {
			var colorBack = ds.getColumn(i, "RESULT_COLOR");
			
			// 승리
			if (colorBack == "#28344e") {
					ds.setColumn(i, "RESULT_COLOR", "#8c95b4");
					ds.setColumn(i, "RESULT_BAR", "#5d63cd");
			} 
			// 패배
			else if (colorBack == "#59343b") {
					ds.setColumn(i, "RESULT_COLOR", "#8c758a");
					ds.setColumn(i, "RESULT_BAR", "#c63b45");
			}
		}
		
		
    } else {
        nexacro.loadStyle("xcssrc::theme_black.xcss");
		
		this.list_statistics.setCellProperty("body", "cell_devide", "background", 
			"#1f2326");
		this.list_role.setCellProperty("body", "CellLine", "background", 
			"#1f2326");
		this.list_agent.setCellProperty("body", "CellLine", "background", 
			"#1f2326");
		this.list_weapon.setCellProperty("body", "CellLine", "background", 
			"#1f2326");
		
		var ds = this.ds_list;
		
		for (var i = 0; i < ds.getRowCount(); i++) {
			var colorBack = ds.getColumn(i, "RESULT_COLOR");
			
			// 승리
			if (colorBack == "#8c95b4") { /*7a9ac2*/
					ds.setColumn(i, "RESULT_COLOR", "#28344e");
					ds.setColumn(i, "RESULT_BAR", "#5383e8");
			} 
			// 패배
			else if (colorBack == "#8c758a") {
					ds.setColumn(i, "RESULT_COLOR", "#59343b");
					ds.setColumn(i, "RESULT_BAR", "#e84057");
			}
		}
		
    }
    this.resetScroll();
};


// 역할군 리스트뷰 다시 그리기
this.fn_resizeRoleList = function()
{
    var nRowHeight = 80;
    
    var nTotalCount = this.ds_role.getRowCount();
    var nNewHeight = nTotalCount * nRowHeight;
    if (nNewHeight == 0) { // 값이 없으면 높이 0
		nNewHeight = 0;
	}
    
    this.list_role.height = nNewHeight;
    this.resetScroll();
};

// 요원 리스트뷰 다시 그리기
this.fn_resizeAgentList = function()
{
    var nRowHeight = 80;
    
    var nTotalCount = this.ds_agent.getRowCount();
    var nNewHeight = nTotalCount * nRowHeight;
    if (nNewHeight == 0) { // 값이 없으면 높이 0
		nNewHeight = 0;
	}
    
    this.list_agent.height = nNewHeight;
    this.resetScroll();
};

// 무기 리스트뷰 다시 그리기
this.fn_resizeWeaponList = function()
{
    var nRowHeight = 80;
    
    var nTotalCount = this.ds_weapon.getRowCount();
    var nNewHeight = nTotalCount * nRowHeight;
    if (nNewHeight == 0) { // 값이 없으면 높이 0
		nNewHeight = 0;
	}
    
    this.list_weapon.height = nNewHeight;
    this.resetScroll();
};

// 전적 리스트뷰 다시 그리기
this.fn_resizeMatchList = function()
{
    var nRowHeight = 150;
    
    var nTotalCount = this.ds_list.getRowCount();
    var nNewHeight = nTotalCount * nRowHeight;
    if (nNewHeight == 0) { // 값이 없으면 높이 0
		nNewHeight = 0;
	}
    
    this.list_match_list.height = nNewHeight;
	
	this.fn_setThemeCss();
	
    this.resetScroll();
};

// 트랜잭션 콜백
this.fn_Callback = function(svcId, errCode, errMsg)
{
    if(errCode < 0) {
		this.gfnAlert("msg.notice.search_failed", [errMsg], "nodata"); //////////////////
        return;
    }
    // 요원 정보 조회
    if (svcId == "getAgents") {
		// 아무 동작 안해도 괜찮음
    }
	// 티어 조회
    if (svcId == "searchTier") {
		// 여기서 해야하는데 전적검색 로딩 다 되면 그때 출력
	}
	else if (svcId == "getSavedList") {
		
		// 데이터가 제대로 왔는지 개수 확인
		var nSavedCount = this.ds_saved_ids.getRowCount();
		
		// 데이터가 없으면 중단
		if (nSavedCount == 0) return;
		
		// 매칭 시작
		for(var i=0; i<this.ds_list.getRowCount(); i++)
		{
			var sMatchId = this.ds_list.getColumn(i, "MATCH_ID");
			var sStarCss = "star_empty"; 
			
			// 검색 시도
			var nFindRow = this.ds_saved_ids.findRow("MATCH_ID", sMatchId);
			
			if(nFindRow >= 0) {
				sStarCss = "star_fill"; 
			}
			
			this.ds_list.setColumn(i, "STAR_CSS", sStarCss);
		}
		
		// 강제 갱신
		this.list_match_list.redraw();
	}
	
	// 전적 조회
    else if (svcId == "searchMatch") {
		
		this.ds_list.addColumn("STAR_CSS", "string");
		
		this.list_match_list.enableevent = false; // 그리기 일시 중지
		for(var i=0; i<this.ds_list.getRowCount(); i++)
        {
            var sMatchId = this.ds_list.getColumn(i, "MATCH_ID");
            var sStarCss = "star_empty";
            
            // ds_saved_ids 안에 해당 MATCH_ID가 있다면
            if(this.ds_saved_ids.findRow("MATCH_ID", sMatchId) >= 0) {
                sStarCss = "star_fill";
            }
            
            // 데이터셋에 값을 넣음
            this.ds_list.setColumn(i, "STAR_CSS", sStarCss);
        }
		
        this.list_match_list.enableevent = true; // 그리기
		
		
		this.list_agent.setCellProperty("body", "CellAgentIcon", "background", 
			"expr:\"url('\" + AGENT_ICON + \"') no-repeat center center /contain\"");
		
		this.list_role.setCellProperty("body", "CellRoleIcon", "background", 
			"expr:\"url('\" + ROLE_ICON + \"') no-repeat center center /contain\"");
		
		this.list_weapon.setCellProperty("body", "CellWeaponIcon", "background", 
			"expr:\"url('\" + WEAPON_ICON + \"') no-repeat center center /contain\"");
		
		this.list_match_list.setCellProperty("body", "cell_match_agent_icon", "background", 
			"expr:\"url('\" + AGENT_ICON + \"') no-repeat center center /contain\"");
		
		this.list_match_list.setCellProperty("body", "cell_rank", "background", 
			"expr: RANK == '1' ? '#eb9c00' : (RANK == '2' ? '#7d59ea' : '#57646f')");
		
		var Most_Agent = "url('"+ this.ds_agent.getColumn(0,"AGENT_FULL_BODY") +"') no-repeat center center /contain";
		this.div_user_status.form.Logo_CHAMBER.background = Most_Agent;
		
		// 왼쪽 리스트뷰 다시 그리기
        this.fn_resizeRoleList();
        this.fn_resizeAgentList();
		this.fn_resizeWeaponList();
		this.fn_resizeMatchList();
		
		// 차트와 승패 박스 그리기
		this.fn_drawRecentHistory(10);
        this.fnCreateChart();
		
        // 검색 중 화면 숨기기
		if(this.ds_list.getRowCount()){
			this.sta_hide_screen.visible = false;
			this.btn_space_2.visible = false;
			
			// 여기서 티어 노출
			if (this.ds_tier.rowcount > 0) {
				var tier     = this.ds_tier.getColumn(0, "TIER_NAME");
				var rr       = this.ds_tier.getColumn(0, "RR");
				var tierIcon = this.ds_tier.getColumn(0, "TIER_ICON");
				var wins     = this.ds_tier.getColumn(0, "SEASON_WINS");
				var total    = this.ds_tier.getColumn(0, "SEASON_GAMES");
				var winRate  = this.ds_tier.getColumn(0, "SEASON_WIN_RATE");
				
				var Statustext = "현재 티어: " + tier + " (" + rr + " 포인트)\n";
				Statustext += "이번 시즌: " + wins + "승 / " + total + "전 (" + winRate + "%)";
				
				this.div_user_status.form.sta_user_status.text = Statustext;
				if(tierIcon) {
					this.div_user_status.form.sta_user_status.background = "url('"+tierIcon+"') no-repeat left center /contain";
				}
			} else {
				this.div_user_status.form.sta_user_status.text = "티어 정보 없음";
				this.div_user_status.form.sta_user_status.background = "";
			}
			this.div_user_status.form.resetScroll();
		}
		else{
			this.sta_hide_screen.text = "최근 전적 정보 없음"
			this.div_user_status.form.sta_user_status.text = "티어 정보 없음";
			this.btn_new_search_2.visible = "true";
			this.div_user_status.form.resetScroll();
		}
		this.resetScroll();
        this.scrollbartype = "none auto";
    }
};

// 승패 박스 그리기
this.fn_drawRecentHistory = function(nLimit)
{
    var nTotal = this.ds_list.getRowCount();
    
    // 데이터가 없으면 리턴
    if (nTotal == 0) return;
    
    // 표시할 개수 제한 (데이터가 10개보다 적으면 전체 표시)
    var nCount = (nTotal < nLimit) ? nTotal : nLimit;
	
    // 카운팅 변수
    var nWins = 0;
    var nDraws = 0;
    var nLosses = 0;
    
    // 기존에 그려진 컴포넌트 초기화
    // div_recent_history 내부의 모든 동적 컴포넌트 삭제
    var comps = this.div_recent_history.form.components;
    for (var i = comps.length - 1; i >= 0; i--) {
        var comp = comps[i];
        // 동적으로 만든 컴포넌트만 삭제 (ID에 "sta_result_"가 포함된 것)
        if (comp.id.indexOf("sta_result_") > -1) {
            this.div_recent_history.form.removeChild(comp.id);
            comp.destroy();
			comp = null;
        }
    }
	
    //박스 그리기 설정 값
	var nLeft = 200;
    var nTop  = 150;     // 시작 위치
    var nWidth = 40;    // 박스 너비
    var nHeight = 40;   // 박스 높이
    var nGap = 10;       // 박스 간격
	
    // 반복문으로 데이터 만큼 박스 생성
    for (var i = 0; i < nCount; i++) {
        
        // 데이터셋에서 승패 여부 가져오기 
        var sResult = this.ds_list.getColumn(i, "RESULT");
        
		var sId = "sta_result_" + i;
		var objStatic = new Static();
		objStatic.init(sId, nLeft, nTop, nWidth, nHeight);
		// 스타일 설정
        objStatic.textAlign = "center";
        objStatic.verticalAlign = "middle";
		objStatic.padding = "5px 0px 0px 0px";
        objStatic.font = "20px/normal AppleSDGothicNeoEB00";
        objStatic.borderRadius = "4px";
        
        if (sResult == "승리") {
            nWins++;
			
			objStatic.text = "W";
			objStatic.color = "#81acff";
			objStatic.background = "#2f436e";
        } else if (sResult == "무승부") {
            nDraws++;
			objStatic.text = "D";
			objStatic.color = "#a0a0a0";
			objStatic.background = "#606060";
        } else {
            nLosses++;
			objStatic.text = "L";
			objStatic.color = "#ff6c76";
			objStatic.background = "#703c47";
        }
        
        // 화면에 추가 및 표시
        this.div_recent_history.form.addChild(sId, objStatic);
        objStatic.show();
        
        // 다음 박스 위치 계산
        nLeft = nLeft + nWidth + nGap;
    }
	
    // 텍스트 요약 정보 표시 (예: "10전 4승 0무 6패")
	var nRate = Math.round((nWins / nCount) * 100);
    var sSummary = nCount + "전 " + nWins + "승 " + nDraws + "무 " + nLosses + "패";
	this.div_recent_history.form.sta_hole.text = nRate+"%";
	
    this.div_recent_history.form.sta_recent_summary.text = sSummary;
	
	// 1번째 : 승리
	var nRowWin = this.ds_chart_win_rate.addRow();
	this.ds_chart_win_rate.setColumn(nRowWin, "RATE", nWins);
	
	// 2번째 : 무승부
	var nRowLoss = this.ds_chart_win_rate.addRow();
	this.ds_chart_win_rate.setColumn(nRowLoss, "RATE", nDraws);
	
	// 3번째 : 패배
	var nRowDraw = this.ds_chart_win_rate.addRow();
	this.ds_chart_win_rate.setColumn(nRowDraw, "RATE", nLosses);
	
	this.div_recent_history.form.resetScroll();
};

// 차트 그리기
this.fnCreateChart = function() {
	var chart = this.div_recent_history.form.chart_win_rate;	// Chart id
	var canvas = chart.getCanvas();								// Canvas 가져오기
	var cvs = canvas.id;										// Canvas id
	var ds = this.ds_chart_win_rate;							// bind Dataset
	
	DxChart.reset(canvas);										// 차트영역(Canvas) reset
	
	// Chart 그리기  DxChartPie - Pie(파이) chart  
	var chartObj = new DxChartPie({
			id: cvs,							// 차트 canvas의 아이디
			elem: canvas,						// 차트 canvas 오브젝트
			binddataset: ds,					// Binddataset
			data:["bind:RATE"],					// 차트에 표현할 데이터 리스트 ["bind:Col0","bind:Col1"]
			options: this.fnGetOption(canvas) 	// 차트에 대한 옵션값 {background:() , key:() , xaxis , yaxis , tooltip }  : 	
		}).roundRobin();
};

this.fnGetOption = function (canvas, background)
{
	var Option = {
		radius:75,
		CustomData: ['bind:RATE'],
		shadow: { Use : false },
		labels: "",
		title:  "",
		background:{ GridVlines: false, GridBorder: true },
		tooltips: "",
		key: "",
		colors: ["#4a76d2","#6e6e86","#e74056"],
		colorsStroke: 'transparent',
	}
	return Option;
};
this.div_recent_history_btn_new_search_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var sMsgId = "msg.notice.new_search";	//메세지ID
	var arrArg = "";						//메세지취환될값 배열[생략가능]
	var sPopId = "";
	// function 전달
	this.gfnAlert(sMsgId, arrArg, sPopId, function(strId, strVal) {
			if (strVal == true) {
				// 확인 선택
				var objWorkFrame = this.fvApp.mainframe.VFrameSet00.WorkFrame;
				objWorkFrame.formurl = "Valorant::valo1001fm.xfdl";
			}
			else {
				// 취소 선택
			}
		});
};

var count = 0;
this.list_match_list_oncellclick = function(obj:nexacro.ListView,e:nexacro.ListViewClickEventInfo)
{
	if(e.cellid == 'cell_star')
	{
		
		// 로그인 체크 (ds_login에 데이터가 없으면 로그인 안 된 것으로 간주)
        if (this.fvApp.gds_login.getRowCount() == 0) {
			this.gfnAlert("msg.notice.login4", "", "nodata"); //////////////////
            return;
        }
		
		this.ds_list_copy.clearData();
		
		var nRow = this.ds_list_copy.addRow();
		this.ds_list_copy.copyRow(nRow, this.ds_list, e.row);
		
		var sUserDiscordId = this.fvApp.gds_login.getColumn(0, "DISCORD_ID");
		var sUserDiscordName = this.fvApp.gds_login.getColumn(0, "USER_NAME");
		this.ds_list_copy.setColumn(nRow, "DISCORD_ID", sUserDiscordId);
        this.ds_list_copy.setColumn(nRow, "USER_NAME", sUserDiscordName);
		
		// 현재 상태값 가져오기 (데이터셋 기준)
        var sCurrentCss = this.ds_list.getColumn(e.row, "STAR_CSS");
		
		if ( sCurrentCss == "star_empty" ) {		
			this.ds_list.setColumn(e.row, "STAR_CSS", "star_fill");
			// 전적 저장 transaction
			var strsvcid        = "savematch";
			var strurl          = "SvcUrl::savematch.do";
			var strindatasets   = "in_list=ds_list_copy";
			var stroutdatasets  = "";
			var strargument     = "";
			var strcallbackfunc = "fn_Callback";
			
			this.transaction(strsvcid, strurl, strindatasets, stroutdatasets, strargument, strcallbackfunc);
		}
		else{
			// 전적 삭제 transaction
			this.ds_list.setColumn(e.row, "STAR_CSS", "star_empty");
			
			var strsvcid        = "deletematch";
			var strurl          = "SvcUrl::deletematch.do";
			var strindatasets   = "in_list=ds_list_copy";
			var stroutdatasets  = "";
			var strargument     = "";
			var strcallbackfunc = "fn_Callback";
			
			this.transaction(strsvcid, strurl, strindatasets, stroutdatasets, strargument, strcallbackfunc);
		}
	}
};

this.list_match_list_onmousemove = function(obj:nexacro.ListView,e:nexacro.ListViewMouseEventInfo)
{
	if(e.cellid == 'cell_star')
	{	
		this.list_match_list.cursor = "pointer";
	}
	else{
		this.list_match_list.cursor = "default";
	}
};]]></Script>
